<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2A4B5E"
    tools:context=".Homepage">

  <TextView
      android:id="@+id/textView"
      android:layout_width="667dp"
      android:layout_height="79dp"
      android:text="Smile HaPPy Restaurant"
      android:textColor="#FBF8F8"
      android:textSize="60sp"
      tools:layout_editor_absoluteX="333dp"
      tools:layout_editor_absoluteY="52dp" />

  <Button
      android:id="@+id/button"
      android:layout_width="192dp"
      android:layout_height="90dp"
      android:background="#FFFFFF"
      android:text="Add Order >"
      android:textColor="#000000"
      android:textSize="20sp"
      app:iconTint="#267976"
      tools:layout_editor_absoluteX="1035dp"
      tools:layout_editor_absoluteY="295dp" />

  <Button
      android:id="@+id/button2"
      android:layout_width="192dp"
      android:layout_height="90dp"
      android:text="Kitchen >"
      android:textColor="#000000"
      android:textSize="20sp"
      tools:layout_editor_absoluteX="1035dp"
      tools:layout_editor_absoluteY="447dp" />

  <TextView
      android:id="@+id/textView2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Active Order(s)"
      android:textColor="#FFFFFF"
      android:textSize="34sp"
      tools:layout_editor_absoluteX="55dp"
      tools:layout_editor_absoluteY="165dp" />

  <FrameLayout
      android:layout_width="917dp"
      android:layout_height="533dp"
      android:layout_marginStart="55dp"
      android:background="#267976"
      app:layout_constraintStart_toStartOf="parent"
      tools:layout_editor_absoluteY="221dp">

    <TableRow
        android:id="@+id/TableNo"
        android:layout_width="106dp"
        android:layout_height="match_parent">

    </TableRow>

    <TableRow
        android:id="@+id/Food"
        android:layout_width="314dp"
        android:layout_height="match_parent">

    </TableRow>

    <TableRow
        android:id="@+id/Quantity"
        android:layout_width="517dp"
        android:layout_height="match_parent">

    </TableRow>

    <TableRow
        android:id="@+id/OrderTime"
        android:layout_width="678dp"
        android:layout_height="match_parent">

    </TableRow>

    <TableRow
        android:id="@+id/Status"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

  </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>