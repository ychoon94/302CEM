<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/list_background"
        android:layout_width="wrap_content"
        android:layout_height="417dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="218dp"
        android:layout_marginEnd="68dp"
        android:layout_marginStart="62dp"
        android:src="@drawable/list_background" />

    <ImageView
        android:id="@+id/ready_button"
        android:layout_width="268dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/list_background"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/list_background"
        android:layout_marginBottom="167dp"
        android:layout_marginEnd="70dp"
        android:layout_marginStart="883dp"
        android:layout_marginTop="153dp"
        android:src="@drawable/ready_button" />

    <TextView
        android:id="@+id/ready_text"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ready_button"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/ready_button"
        android:layout_alignTop="@+id/ready_button"
        android:layout_marginBottom="15dp"
        android:layout_marginEnd="217dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/roboto_bold"
        android:text="Ready"
        android:textColor="#FFFFFF"
        android:textSize="35sp" />

    <TextView
        android:id="@+id/time_ordered"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/list_background"
        android:layout_alignStart="@+id/list_background"
        android:layout_alignTop="@+id/list_background"
        android:layout_marginBottom="255dp"
        android:layout_marginEnd="866dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="92dp"
        android:fontFamily="@font/roboto_bold"
        android:text="time_ordered"
        android:textColor="#000000"
        android:textSize="38sp" />

    <TextView
        android:id="@+id/table_num"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/list_background"
        android:layout_alignStart="@+id/list_background"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:layout_marginBottom="158dp"
        android:layout_marginEnd="769dp"
        android:layout_marginStart="221dp"
        android:layout_marginTop="185dp"
        android:fontFamily="@font/roboto_bold"
        android:text="tab_num"
        android:textColor="#000000"
        android:textSize="35sp" />

    <TextView
        android:id="@+id/table_no_text"
        android:layout_width="161dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/table_num"
        android:layout_alignStart="@+id/list_background"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:layout_marginBottom="164dp"
        android:layout_marginEnd="173dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="182dp"
        android:fontFamily="@font/roboto_bold"
        android:text="Table No."
        android:textColor="#000000"
        android:textSize="35sp" />

    <TextView
        android:id="@+id/dividing_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignStart="@+id/table_num"
        android:layout_alignTop="@+id/list_background"
        android:layout_marginBottom="326dp"
        android:layout_marginStart="434dp"
        android:layout_marginTop="187dp"
        android:fontFamily="@font/roboto_bold"
        android:text="|"
        android:textColor="#000000"
        android:textSize="36sp" />

    <TextView
        android:id="@+id/total_placed_text"
        android:layout_width="227dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/list_background"
        android:layout_alignStart="@+id/dividing_line"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:layout_marginBottom="160dp"
        android:layout_marginEnd="494dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="186dp"
        android:fontFamily="@font/roboto_bold"
        android:text="Total Placed: "
        android:textColor="#000000"
        android:textSize="35sp" />

    <TextView
        android:id="@+id/item_quantity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignStart="@+id/list_background"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:layout_marginBottom="81dp"
        android:layout_marginEnd="173dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="272dp"
        android:fontFamily="@font/roboto_bold"
        android:text="no.x"
        android:textColor="#FF0800"
        android:textSize="36sp" />

    <TextView
        android:id="@+id/food_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignStart="@+id/item_quantity"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:layout_marginBottom="84dp"
        android:layout_marginEnd="173dp"
        android:layout_marginStart="99dp"
        android:layout_marginTop="269dp"
        android:fontFamily="@font/roboto_bold"
        android:text="food item"
        android:textColor="#FF0800"
        android:textSize="36sp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal" />

</RelativeLayout>