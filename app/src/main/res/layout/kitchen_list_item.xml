<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:id="@+id/orderPlacedLayout">


    <ImageView
        android:id="@+id/list_background"
        android:layout_width="1252dp"
        android:layout_height="345dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:src="@drawable/list_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/btn_ready"
        android:layout_width="268dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/list_background"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/list_background"
        android:src="@drawable/ready_button"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintEnd_toEndOf="@+id/list_background"
        app:layout_constraintHorizontal_bias="0.896"
        app:layout_constraintStart_toStartOf="@+id/list_background"
        app:layout_constraintTop_toTopOf="@+id/list_background"
        app:layout_constraintVertical_bias="0.45" />

    <TextView
        android:id="@+id/ready_text"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btn_ready"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/btn_ready"
        android:layout_alignTop="@+id/btn_ready"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/btn_ready"
        android:textColor="#FFFFFF"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_ready"
        app:layout_constraintEnd_toEndOf="@+id/btn_ready"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="@+id/btn_ready"
        app:layout_constraintTop_toTopOf="@+id/btn_ready"
        app:layout_constraintVertical_bias="0.418" />

    <TextView
        android:id="@+id/time_ordered"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/list_background"
        android:layout_alignStart="@+id/list_background"
        android:layout_alignTop="@+id/list_background"
        android:layout_marginStart="100dp"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/time_ordered"
        android:textColor="#000000"
        android:textSize="38sp"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintStart_toStartOf="@+id/list_background"
        app:layout_constraintTop_toTopOf="@+id/list_background"
        app:layout_constraintVertical_bias="0.142" />

    <TextView
        android:id="@+id/table_num"
        android:layout_width="47dp"
        android:layout_height="45dp"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/list_background"
        android:layout_alignStart="@+id/list_background"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/table_num"
        android:textColor="#000000"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintEnd_toEndOf="@+id/dividing_line"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/table_no_text"
        app:layout_constraintTop_toTopOf="@+id/list_background"
        app:layout_constraintVertical_bias="0.43" />

    <TextView
        android:id="@+id/table_no_text"
        android:layout_width="154dp"
        android:layout_height="47dp"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/table_num"
        android:layout_alignStart="@+id/list_background"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:layout_marginStart="100dp"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/table_number"
        android:textColor="#000000"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintStart_toStartOf="@+id/list_background"
        app:layout_constraintTop_toTopOf="@+id/list_background"
        app:layout_constraintVertical_bias="0.426" />

    <TextView
        android:id="@+id/dividing_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignStart="@+id/table_num"
        android:layout_alignTop="@+id/list_background"
        android:fontFamily="@font/roboto_bold"
        android:text="|"
        android:textColor="#000000"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintEnd_toEndOf="@+id/list_background"
        app:layout_constraintHorizontal_bias="0.242"
        app:layout_constraintStart_toStartOf="@+id/list_background"
        app:layout_constraintTop_toTopOf="@+id/list_background"
        app:layout_constraintVertical_bias="0.422" />

    <TextView
        android:id="@+id/total_placed_text"
        android:layout_width="210dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignEnd="@+id/list_background"
        android:layout_alignStart="@+id/dividing_line"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/total_placed"
        android:textColor="#000000"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintEnd_toEndOf="@+id/list_background"
        app:layout_constraintHorizontal_bias="0.31"
        app:layout_constraintStart_toStartOf="@+id/list_background"
        app:layout_constraintTop_toTopOf="@+id/list_background"
        app:layout_constraintVertical_bias="0.433" />

    <TextView
        android:id="@+id/totalNoPlaced"
        android:layout_width="48dp"
        android:layout_height="47dp"
        android:layout_centerVertical="true"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/table_num"
        android:textColor="#000000"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintEnd_toEndOf="@+id/list_background"
        app:layout_constraintHorizontal_bias="0.01"
        app:layout_constraintStart_toEndOf="@+id/total_placed_text"
        app:layout_constraintTop_toTopOf="@+id/list_background"
        app:layout_constraintVertical_bias="0.426" />

    <TextView
        android:id="@+id/item_quantity"
        android:layout_width="23dp"
        android:layout_height="46dp"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignStart="@+id/list_background"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:layout_marginStart="56dp"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/total_placed_quantity"
        android:textColor="#FF0800"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintEnd_toStartOf="@+id/food_item"
        app:layout_constraintHorizontal_bias="0.62"
        app:layout_constraintStart_toStartOf="@+id/list_background"
        app:layout_constraintTop_toTopOf="@+id/list_background"
        app:layout_constraintVertical_bias="0.712" />

    <TextView
        android:id="@+id/food_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/list_background"
        android:layout_alignStart="@+id/item_quantity"
        android:layout_alignTop="@+id/list_background"
        android:layout_centerVertical="true"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/food_ordered"
        android:textColor="#FF0800"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintEnd_toEndOf="@+id/list_background"
        app:layout_constraintHorizontal_bias="0.144"
        app:layout_constraintStart_toStartOf="@+id/list_background"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.736" />

    <TextView
        android:id="@+id/symbol_x"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_bold"
        android:text="x"
        android:textColor="#FF0800"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="@+id/list_background"
        app:layout_constraintEnd_toStartOf="@+id/food_item"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/item_quantity"
        app:layout_constraintTop_toBottomOf="@+id/table_no_text"
        app:layout_constraintVertical_bias="0.319" />


</androidx.constraintlayout.widget.ConstraintLayout>